<samba:parameter name="web port"
                 context="G"
				 type="list"
                 advanced="1" developer="1"
                 xmlns:samba="http://www.samba.org/samba/DTD/samba-doc">
<description>
  <para>
    Samba の Web サーバーが待ち受けるポートを指定する。
  </para>
</description>

<value type="default">901</value>
<value type="example">80</value>
</samba:parameter>
