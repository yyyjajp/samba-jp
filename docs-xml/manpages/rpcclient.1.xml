                <varlistentry><term>getcoreprinterdrivers</term><listitem><para>コアプリンタドライバの取得</para></listitem></varlistentry>
                <varlistentry><term>enumpermachineconnections</term><listitem><para>マシン接続毎に一覧表示</para></listitem></varlistentry>
